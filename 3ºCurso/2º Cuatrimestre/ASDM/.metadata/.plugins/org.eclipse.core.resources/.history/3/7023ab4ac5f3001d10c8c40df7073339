private boolean isOwnView(org.eclipse.gmf.runtime.notation.View view) {
	return Asdm.diagram.edit.parts.DiagramaEditPart.MODEL_ID.equals(Asdm.diagram.part.AsdmVisualIDRegistry.getModelID(view));
}
	
